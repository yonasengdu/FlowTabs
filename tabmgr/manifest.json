{
  "manifest_version": 3,
  "name": "Alt+Q Tab Switcher (Final)",
  "version": "2.0",
  "description": "Mimics Alt+Tab using Alt+Q to switch between recent tabs.",
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "alt-q-switch": {
      "suggested_key": {
        "default": "Alt+Q"
      },
      "description": "Activate Tab Switcher"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["switcher.css"],
      "matches": ["<all_urls>"]
    }
  ]
}